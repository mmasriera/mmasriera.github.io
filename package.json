  "bugs": {
    "url": "https://github.com/mmasriera/mmasriera.github.io/issues"
  },
  "homepage": "https://github.com/mmasriera/mmasriera.github.io#readme",
  "devDependencies": {
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "node-sass": "^3.9.3"
  }
}
