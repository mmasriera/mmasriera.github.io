  "description": "my page",
  "scripts": {
    "styles-less": "compiledCSS=$(lessc --clean-css style.less) && sed -i '' \"7s/.*/\t\t\t$compiledCSS/\" index.html",
    "styles-sass": "compiledCSS=$(node-sass --output-style compressed style.scss) && sed -i '' \"7s/.*/\t\t\t$compiledCSS/\" index.html"
  },
